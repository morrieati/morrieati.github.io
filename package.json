{
  "name": "gh-pages",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://blog.hahahahahaha.ga",
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "dateformat": "^3.0.3",
    "esm": "^3.2.25",
    "gh-pages": "^2.2.0",
    "highlight.js": "^9.18.1",
    "markdown-toc": "^1.2.0",
    "node-sass": "^4.13.1",
    "pubsub-js": "^1.8.0",
    "query-string": "^6.11.1",
    "querystring-browser": "^1.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "remarkable": "^2.0.0",
    "remarkable-meta": "^1.0.1",
    "unlazy-loader": "^0.1.3"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -b master -d build",
    "genarticles": "node -r esm ./src/utils/article.js && git add .",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build && react-snapshot",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run genarticles",
      "pre-push": "npm run deploy"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "customize-cra": "^0.9.1",
    "husky": "^4.2.3",
    "react-app-rewired": "^2.1.5",
    "react-snapshot": "^1.3.0"
  },
  "reactSnapshot": {
    "include": [
      "/page",
      "/404"
    ],
    "snapshotDelay": 300
  }
}
